<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="rootPane" styleClass="dependent-details-view-header-root-pane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
     <children>
          <HBox prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER" styleClass="dependent-details-view-header-external-pane" >
               <children>
                    <StackPane fx:id="iconContainer" styleClass="dependent-details-view-icon-root-pane" prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER" />
                    <VBox alignment="CENTER" prefWidth="-1.0" HBox.hgrow="ALWAYS">
                         <children>
                              <StackPane fx:id="locationContainer" styleClass="dependent-details-view-header-location-root-pane" />
                              <StackPane fx:id="identityContainer" styleClass="dependent-details-view-header-identity-root-pane" />
                              <StackPane fx:id="actionsContainer" styleClass="dependent-details-view-header-actions-root-pane" prefWidth="-1.0" />
                         </children>
                    </VBox>
                    <StackPane fx:id="contextMenuContainer" prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER" />
               </children>
          </HBox>
     </children>
</StackPane>