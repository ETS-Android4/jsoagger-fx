<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<VBox fx:id="rootPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" id="maximizedDetailsViewRootPane" styleClass="ep-maximize-details-view-root-pane">
		<StackPane>
			<StackPane fx:id="headerAreaSection" id="maximizedDetailsViewPaneHeaderSection" styleClass="ep-details-view-header" />
		</StackPane>
		
		<StackPane fx:id="maximizedDetailsViewPaneVbox" id="maximizedDetailsViewPaneVbox" VBox.vgrow="ALWAYS">
			<HBox fx:id="topAnchored">
				<StackPane fx:id="leftSpacer" id="maximizedDetailsViewLeftSpacer" />
				<VBox fx:id="leftFixedAreaSection" prefHeight="-1.0" prefWidth="-1.0" />
				<ScrollPane fitToHeight="true" fitToWidth="true" fx:id="mainScrollPane" hbarPolicy="NEVER" vbarPolicy="AS_NEEDED">
					<content>
						<HBox style="-fx-padding:8">
							<StackPane fx:id="centerFixedAreaSection" BorderPane.alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" />
							<StackPane fx:id="rightFixedAreaSection" BorderPane.alignment="TOP_RIGHT" prefHeight="-1.0" prefWidth="-1.0" />
						</HBox>
					</content>
				</ScrollPane>
				<StackPane fx:id="rightSpacer" id="maximizedDetailsViewRightSpacer" />
			</HBox>
		</StackPane>
</VBox>

