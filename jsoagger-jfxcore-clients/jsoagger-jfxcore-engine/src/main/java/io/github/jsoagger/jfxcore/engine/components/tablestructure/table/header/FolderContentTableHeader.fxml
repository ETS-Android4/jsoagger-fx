<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>

<StackPane fx:id="tableHeaderRootPane" styleClass="ep-table-header-root-container" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" alignment="CENTER">
    <children>
        <VBox prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER" styleClass="ep-table-header-title-external-container">
            <children>
                <Label fx:id="headerTitle" text="" />
                <VBox alignment="CENTER_LEFT" prefWidth="-1.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" style="-fx-padding:0">
                    <children>
                        <FlowPane alignment="BOTTOM_LEFT" fx:id="titleContainer" hgap="4" vgap="4" VBox.vgrow="ALWAYS" style="-fx-padding:8"/>
                        <HBox fx:id="filterContainer" styleClass="ep-table-header-footer-filter-container" alignment="CENTER_LEFT">
                            <StackPane fx:id="actionsContainer" styleClass="ep-table-header-footer-action-container" alignment="CENTER_LEFT" style="-fx-padding:4 0 0 0;-fx-pref-height:52"/>
                        </HBox>
                        <HBox style="-fx-padding:0 8 0 8;" alignment="CENTER_LEFT" VBox.vgrow="ALWAYS">
                            <TextField fx:id="filterTextField" styleClass="ep-table-filter-text-field" />
                        </HBox>
                    </children>
                </VBox>
            </children>
        </VBox>
    </children>
</StackPane>