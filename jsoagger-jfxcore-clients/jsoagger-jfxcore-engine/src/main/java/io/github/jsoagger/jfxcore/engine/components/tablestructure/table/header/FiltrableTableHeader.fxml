<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<StackPane fx:id="tableHeaderRootPane" styleClass="ep-table-header-root-container" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
		<HBox prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER" styleClass="ep-table-header-title-external-container">
			<StackPane fx:id="iconContainer" styleClass="ep-table-header-icon-container" />
			<VBox alignment="CENTER" prefWidth="-1.0" HBox.hgrow="ALWAYS" spacing="8">
				<HBox fx:id="titleContainer" VBox.vgrow="ALWAYS" styleClass="ep-table-header-title-container">
					<Label fx:id="title" text="" styleClass="ep-table-header-main-label" />
					<Label fx:id="itemsCount" text="" styleClass="ep-table-header-count-label" />
				</HBox>

				<HBox fx:id="actionsAndFiltersContainer" styleClass="ep-table-header-footer-container" alignment="CENTER_LEFT" spacing="16">
					<StackPane fx:id="filterContainer" HBox.hgrow="ALWAYS" alignment="CENTER_LEFT">
					<TextField fx:id="filterTextField" styleClass="ep-table-filter-text-field" />
					</StackPane>
				</HBox>
				<StackPane VBox.vgrow="ALWAYS" />
			</VBox>
		</HBox>
</StackPane>