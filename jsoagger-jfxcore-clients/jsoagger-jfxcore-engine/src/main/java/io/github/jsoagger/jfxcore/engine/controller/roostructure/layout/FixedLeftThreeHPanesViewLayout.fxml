<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.HBox?>

<StackPane fx:id="rootPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
	<VBox fx:id="contentWrapper" BorderPane.alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0">
		<HBox>
			<StackPane fx:id="leftFixedAreaSection" BorderPane.alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" />
			<ScrollPane fitToHeight="true" fitToWidth="true" fx:id="mainScrollPane" hbarPolicy="AS_NEEDED" vbarPolicy="AS_NEEDED">
				<content>
					<HBox HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
						<StackPane fx:id="centerFixedAreaSection" BorderPane.alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" />
						<StackPane fx:id="rightFixedAreaSection" BorderPane.alignment="TOP_RIGHT" prefHeight="-1.0" prefWidth="-1.0" />
					</HBox>
				</content>
			</ScrollPane>
		</HBox>
	</VBox>
	<HBox fx:id="editorStructureAreaSection" alignment="TOP_CENTER" style="-fx-padding:32 0 0 0;"/>
</StackPane>

